s_sidebar_element
  // 검색
  s_search: section#search.sp-container: form
    input.use-js(type="text" name="[##_search_name_##]" value="[##_search_text_##]" onkeypress="if (event.keyCode == 13) { [##_search_onclick_submit_##] }" placeholder="검색은 여기")
    button.use-js(value="검색" onclick="[##_search_onclick_submit_##]")
      include ../icons/search
